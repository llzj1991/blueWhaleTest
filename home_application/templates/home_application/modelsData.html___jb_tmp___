    <script>
        $(function(){
                function renderTpl(str, cfg) {
                    var re = /(#(.+?)#)/g;

                    return str.replace(re, function() {
                        var val = cfg[arguments[2]]+'';
                        if(typeof val == 'undefined') {
                            val = '';
                        }
                        return val;
                    });
                }
                // 异步请求后台数据
                $.ajax({
                    url: "{% url 'framework:getModelsData' %}",
                    type: 'GET',
                    success: function(res){
                        var _html = ' ';
                        var items = res["items"];
                        var tpl = $('#tbody_tpl_1559099317777').html();
                        var headerTpl =  $('#theader_tpl_1559099317777').html();
                        for (var i=0; i < items.length; i++){
                            var item = items[i];
                            _html = _html+renderTpl(tpl, item);
                        }
                        $('.ranger-box').html(renderTpl(headerTpl,res["catalogues"])+_html);
                    }
                });
                 $("#submitData").click(function(){
                    var datas = $('#form_getHomeData').serialize();
                    $.ajax({
                        url: "{% url 'framework:getModelsData' %}",
                        type: 'post',
                        dataType:'json',
                        data: datas,
                        success: function(res){
                            var _html = ' ';
                            var items = res["items"];
                            var tpl = $('#tbody_tpl_1559099317777').html();
                            var headerTpl =  $('#theader_tpl_1559099317777').html();
                            for (var i=0; i < items.length; i++){
                                var item = items[i];
                                _html = _html+renderTpl(tpl, item);
                            }
                            $('.ranger-box').html(renderTpl(headerTpl,res["catalogues"])+_html);
                         }
                    });
                });
            });
    </script>
