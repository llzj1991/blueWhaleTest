                                            <template id="tbody_tpl_1559099317777">
                                                <tr>
                                                    <td style="width: 10%;text-align: center;">#id#</td>
                                                    <td style="width: 10%;text-align: center;">#name#</td>
                                                    <td style="width: 10%;text-align: center;">#ipCode#</td>
                                                    <td style="width: 10%;text-align: center;">#systemType#</td>
                                                    <td style="width: 10%;text-align: center;">#disk#</td>
                                                    <td style="width: 10%;text-align: center;">#proportion#</td>
                                                    <td style="width: 10%;text-align: center;">#remarks#</td>
